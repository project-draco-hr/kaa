{
  StatisticsService service=StatisticsService.getService();
  assertNotNull(service);
  StatisticsService.setConfig(configMock);
  try {
    Thread.sleep(500);
  }
 catch (  InterruptedException e) {
    fail(e.toString());
  }
  assertTrue(service.isAlive());
  service.shutdown();
  assertTrue(!service.isAlive());
}

{
  StatisticsService service=StatisticsFactory.getService(ChannelType.HTTP);
  assertNotNull(service);
  service.setConfig(configMock);
  try {
    Thread.sleep(500);
  }
 catch (  InterruptedException e) {
    fail(e.toString());
  }
  assertTrue(service.isAlive());
  service.shutdown();
  assertTrue(!service.isAlive());
}

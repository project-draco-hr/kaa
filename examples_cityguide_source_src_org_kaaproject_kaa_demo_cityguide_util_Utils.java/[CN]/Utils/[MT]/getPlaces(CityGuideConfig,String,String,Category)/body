{
  List<City> cities=getCities(config,areaName);
  if (cities != null) {
    for (    City city : cities) {
      if (city.getName().equals(cityName)) {
        List<Place> places=new ArrayList<>();
        for (        Place place : city.getPlaces()) {
          if (place.getCategory() == placeCategory) {
            places.add(place);
          }
        }
        return places;
      }
    }
  }
  return null;
}

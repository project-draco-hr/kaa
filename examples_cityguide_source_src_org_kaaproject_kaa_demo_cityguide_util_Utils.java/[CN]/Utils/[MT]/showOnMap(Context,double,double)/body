{
  String uri=formatLatitudeLongitude("http://maps.google.com/maps?f=q&q=(%f,%f)",latitude,longitude);
  try {
    ComponentName compName=new ComponentName(MAPS_PACKAGE_NAME,MAPS_CLASS_NAME);
    Intent mapsIntent=new Intent(Intent.ACTION_VIEW,Uri.parse(uri)).setComponent(compName);
    context.startActivity(mapsIntent);
  }
 catch (  ActivityNotFoundException e) {
    String url=formatLatitudeLongitude("geo:%f,%f",latitude,longitude);
    Intent mapsIntent=new Intent(Intent.ACTION_VIEW,Uri.parse(url));
    try {
      context.startActivity(mapsIntent);
    }
 catch (    ActivityNotFoundException e2) {
      mapsIntent=new Intent(Intent.ACTION_VIEW,Uri.parse(uri));
      context.startActivity(mapsIntent);
    }
  }
}

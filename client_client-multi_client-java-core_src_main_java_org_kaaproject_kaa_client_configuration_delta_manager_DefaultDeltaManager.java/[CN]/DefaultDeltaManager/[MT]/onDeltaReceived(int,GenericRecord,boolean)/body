{
  ConfigurationDeltaFactory deltaFactory=new DefaultConfigurationDeltaFactory();
  ConfigurationDelta deltaResult=deltaFactory.createDelta(data);
  if (fullResync) {
    if (rootReceiver != null) {
      rootReceiver.loadDelta(deltaResult);
    }
  }
 else {
    DeltaReceiver receiver=receivers.get(deltaResult.getHandlerId());
    if (receiver != null) {
      receiver.loadDelta(deltaResult);
    }
  }
}

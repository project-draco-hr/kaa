{
  StringBuilder sb=new StringBuilder("ThriftOperationsServer(");
  boolean first=true;
  sb.append("id:");
  if (this.id == null) {
    sb.append("null");
  }
 else {
    sb.append(this.id);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("priority:");
  sb.append(this.priority);
  first=false;
  sb.append(")");
  return sb.toString();
}

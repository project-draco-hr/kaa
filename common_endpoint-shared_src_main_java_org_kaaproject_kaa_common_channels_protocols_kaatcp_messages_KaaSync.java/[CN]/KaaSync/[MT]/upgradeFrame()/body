{
switch (getKaaSyncMessageType()) {
case SYNC:
    if (isRequest()) {
      return new SyncRequest(this);
    }
 else {
      return new SyncResponse(this);
    }
case UNUSED:
  throw new KaaTcpProtocolException("KaaSync Message type is incorrect");
}
throw new KaaTcpProtocolException("KaaSync Message type is incorrect");
}

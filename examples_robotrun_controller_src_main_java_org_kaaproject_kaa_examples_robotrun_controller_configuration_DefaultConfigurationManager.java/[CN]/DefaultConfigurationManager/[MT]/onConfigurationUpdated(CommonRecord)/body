{
  LOG.info("onConfigurationUpdated: Received new labyrinth configuration rootConfiguration=" + "{}",rootConfiguration != null ? rootConfiguration : "null");
  labyrinth=labyrinthConverter.convert(labyrinth,rootConfiguration);
  LOG.info("onConfigurationUpdated: Received new labyrinth configuration, labyrinth=" + "{}",labyrinth != null ? labyrinth : "null");
  notifyListeners();
}

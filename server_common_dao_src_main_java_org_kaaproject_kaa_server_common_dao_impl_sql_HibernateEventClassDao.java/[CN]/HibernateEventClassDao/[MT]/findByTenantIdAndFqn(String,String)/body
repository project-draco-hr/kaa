{
  LOG.debug("Find event classes by tenant id [{}] and fqn [{}]",tenantId,fqn);
  List<EventClass> eventClass=null;
  if (isNotBlank(tenantId)) {
    eventClass=findListByCriterionWithAlias(TENANT_PROPERTY,TENANT_ALIAS,Restrictions.and(Restrictions.eq(TENANT_REFERENCE,Long.valueOf(tenantId)),Restrictions.eq(FQN_PROPERTY,fqn)));
  }
  return eventClass;
}

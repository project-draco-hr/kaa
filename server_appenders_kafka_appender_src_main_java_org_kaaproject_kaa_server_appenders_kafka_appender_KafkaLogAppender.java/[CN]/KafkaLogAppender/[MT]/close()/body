{
  LOG.info("Try to stop cassandra log appender...");
  if (!closed) {
    closed=true;
    if (logEventDao != null) {
      logEventDao.close();
    }
    if (executor != null) {
      executor.shutdownNow();
    }
    if (scheduler != null) {
      scheduler.shutdownNow();
    }
  }
  LOG.info("Cassandra log appender stoped.");
}

{
  TTupleProtocol oprot=(TTupleProtocol)prot;
  BitSet optionals=new BitSet();
  if (struct.isSetApplicationEventFamilyMapId()) {
    optionals.set(0);
  }
  oprot.writeBitSet(optionals,1);
  if (struct.isSetApplicationEventFamilyMapId()) {
    oprot.writeString(struct.applicationEventFamilyMapId);
  }
}

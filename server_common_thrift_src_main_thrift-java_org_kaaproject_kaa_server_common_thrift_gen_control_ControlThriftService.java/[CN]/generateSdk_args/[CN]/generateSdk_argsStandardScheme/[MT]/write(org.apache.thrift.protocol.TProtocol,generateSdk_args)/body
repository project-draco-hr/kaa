{
  struct.validate();
  oprot.writeStructBegin(STRUCT_DESC);
  if (struct.sdkPlatform != null) {
    oprot.writeFieldBegin(SDK_PLATFORM_FIELD_DESC);
    oprot.writeI32(struct.sdkPlatform.getValue());
    oprot.writeFieldEnd();
  }
  if (struct.applicationId != null) {
    oprot.writeFieldBegin(APPLICATION_ID_FIELD_DESC);
    oprot.writeString(struct.applicationId);
    oprot.writeFieldEnd();
  }
  oprot.writeFieldBegin(PROFILE_SCHEMA_VERSION_FIELD_DESC);
  oprot.writeI32(struct.profileSchemaVersion);
  oprot.writeFieldEnd();
  oprot.writeFieldBegin(CONFIGURATION_SCHEMA_VERSION_FIELD_DESC);
  oprot.writeI32(struct.configurationSchemaVersion);
  oprot.writeFieldEnd();
  oprot.writeFieldBegin(NOTIFICATION_SCHEMA_VERSION_FIELD_DESC);
  oprot.writeI32(struct.notificationSchemaVersion);
  oprot.writeFieldEnd();
  oprot.writeFieldStop();
  oprot.writeStructEnd();
}

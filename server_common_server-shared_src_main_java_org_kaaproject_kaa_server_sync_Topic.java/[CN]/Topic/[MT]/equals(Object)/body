{
  if (this == o) {
    return true;
  }
  if (o == null || getClass() != o.getClass()) {
    return false;
  }
  Topic topic=(Topic)o;
  if (id != null ? !id.equals(topic.id) : topic.id != null) {
    return false;
  }
  if (name != null ? !name.equals(topic.name) : topic.name != null) {
    return false;
  }
  if (subscriptionType != topic.subscriptionType) {
    return false;
  }
  return true;
}

{
  KaaAuthorityDto autority=KaaAdmin.getAuthInfo().getAuthority();
switch (autority) {
case KAA_ADMIN:
    nodes.add(new TenantsPlace());
  break;
case TENANT_ADMIN:
nodes.add(new ApplicationsPlace());
nodes.add(new UsersPlace());
nodes.add(new EcfsPlace());
break;
case TENANT_DEVELOPER:
nodes.add(new ApplicationsPlace());
break;
case TENANT_USER:
nodes.add(new ApplicationsPlace());
break;
}
}

{
  ActionButtonCell<LogSchemaDto> cell=new ActionButtonCell<LogSchemaDto>(Utils.resources.download(),text,embedded,new ActionListener<LogSchemaDto>(){
    @Override public void onItemAction(    LogSchemaDto value){
      Integer logSchemaVersion=value.getMajorVersion();
      RowActionEvent<String> rowDownloadSchemaEvent=new RowActionEvent<>(String.valueOf(logSchemaVersion),KaaRowAction.DOWNLOAD_LOG_SCHEMA);
      fireEvent(rowDownloadSchemaEvent);
    }
  }
,new ActionValidator<LogSchemaDto>(){
    @Override public boolean canPerformAction(    LogSchemaDto value){
      return !embedded;
    }
  }
);
  Column<LogSchemaDto,LogSchemaDto> column=new Column<LogSchemaDto,LogSchemaDto>(cell){
    @Override public LogSchemaDto getValue(    LogSchemaDto item){
      return item;
    }
  }
;
  return column;
}

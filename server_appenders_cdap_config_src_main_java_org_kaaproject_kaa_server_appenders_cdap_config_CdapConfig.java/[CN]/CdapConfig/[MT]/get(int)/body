{
switch (field$) {
case 0:
    return stream;
case 1:
  return host;
case 2:
return port;
case 3:
return ssl;
case 4:
return verifySslCert;
case 5:
return writerPoolSize;
case 6:
return callbackThreadPoolSize;
case 7:
return version;
case 8:
return authClient;
case 9:
return username;
case 10:
return password;
default :
throw new org.apache.avro.AvroRuntimeException("Bad index");
}
}

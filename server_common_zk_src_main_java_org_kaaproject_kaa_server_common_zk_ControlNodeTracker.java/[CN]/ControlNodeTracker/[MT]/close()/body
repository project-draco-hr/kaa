{
  LOG.info("Closing");
  listeners.clear();
  if (controlCache != null) {
    controlCache.close();
  }
  if (nodePath != null) {
    try {
      client.delete().forPath(nodePath);
      LOG.debug("Node with path {} successfully deleted",nodePath);
    }
 catch (    Exception e) {
      LOG.debug("Failed to delete node",e);
    }
  }
  client.close();
}

{
  super("http://" + url,privateKey,publicKey,remotePublicKey);
  this.endpointRegistrationRequestConverter=new AvroByteArrayConverter<>(EndpointRegistrationRequest.class);
  this.profileUpdateRequestConverter=new AvroByteArrayConverter<>(ProfileUpdateRequest.class);
  this.syncRequestConverter=new AvroByteArrayConverter<>(SyncRequest.class);
  this.longSyncRequestConverter=new AvroByteArrayConverter<>(LongSyncRequest.class);
  this.syncResponseConverter=new AvroByteArrayConverter<>(SyncResponse.class);
}

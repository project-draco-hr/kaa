{
  if (LOG.isDebugEnabled()) {
    String authoritiesString="";
    for (int i=0; i < authorities.length; i++) {
      if (i > 0) {
        authoritiesString+=", ";
      }
      authoritiesString+=authorities[i];
    }
    LOG.debug("Find user by tenant id [{}] and authorities [{}]",id,authoritiesString);
  }
  return findListByCriterionWithAlias(TENANT_PROPERTY,TENANT_ALIAS,Restrictions.and(Restrictions.eq(TENANT_REFERENCE,Long.valueOf(id)),Restrictions.in(AUTHORITY_PROPERTY,KaaAuthorityDto.parseList(authorities))));
}

{
  SupportedChannelRecord suppRecord=new SupportedChannelRecord();
  suppRecord.supportedChannelType=SupportedChannelType.getSupportedChannelTypeFromByte(buffer.get());
  int nameLength=buffer.get() & 0x000000FF;
  suppRecord.port=buffer.getShort();
  byte[] nameBytes=new byte[nameLength];
  buffer.get(nameBytes);
  suppRecord.hostName=new String(nameBytes);
  return suppRecord;
}

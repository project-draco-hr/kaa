{
switch (field$) {
case 0:
    return host;
case 1:
  return port;
case 2:
return ssl;
case 3:
return verifySslCert;
case 4:
return username;
case 5:
return password;
case 6:
return connectionPoolSize;
case 7:
return header;
case 8:
return path;
case 9:
return method;
case 10:
return mimeType;
default :
throw new org.apache.avro.AvroRuntimeException("Bad index");
}
}

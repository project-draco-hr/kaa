{
  SyncResponse response=null;
  int t=rnd.nextInt(5);
switch (t) {
case 0:
    response=generateRedirectionResponse();
  break;
case 1:
response=generateConfResyncResponse();
break;
case 2:
response=generateProfResyncResponse();
break;
case 3:
response=generateDeltaResponse();
break;
case 4:
response=generateNoDeltaResponse();
break;
default :
response=generateRedirectionResponse();
break;
}
return response;
}

{
  if (event != null) {
    event.validate();
  }
  if (route != null) {
    route.validate();
  }
  if (userRoute != null) {
    userRoute.validate();
  }
  if (endpointRouteUpdate != null) {
    endpointRouteUpdate.validate();
  }
  if (endpointStateUpdate != null) {
    endpointStateUpdate.validate();
  }
}

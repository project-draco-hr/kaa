    @Override public String getValue(    ProfileFilterRecordDto item){
      return item.getServerProfileSchemaVersion() != null ? item.getServerProfileSchemaVersion() + "" : "";
    }
  }
,60);
  prefWidth+=super.constructColumnsImpl(table);

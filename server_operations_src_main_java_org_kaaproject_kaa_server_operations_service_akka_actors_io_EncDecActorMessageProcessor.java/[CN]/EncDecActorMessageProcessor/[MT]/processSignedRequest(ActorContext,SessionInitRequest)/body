{
  SyncRequest request=decodeRequest(message);
  EndpointObjectHash key=getEndpointObjectHash(request);
  NettySessionInfo session=new NettySessionInfo(message.getChannelUuid(),message.getChannelContext(),message.getChannelType(),crypt.getDecodedSessionKey(),key,request.getSyncRequestMetaData().getApplicationToken(),message.getKeepAlive());
  message.onSessionCreated(session);
  forwardToOpsActor(context,session,request,message);
}

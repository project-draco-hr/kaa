{
  CommonArray hBordersArray_=commonFactory.createCommonArray((Schema)null,Arrays.asList(createBorderRecord(1,1,BorderType.FREE),createBorderRecord(1,2,BorderType.SOLID)));
  CommonArray vBordersArray_=commonFactory.createCommonArray((Schema)null,Arrays.asList(createBorderRecord(1,1,BorderType.FREE),createBorderRecord(2,1,BorderType.SOLID)));
  CommonValue hBordersArray=commonFactory.createCommonValue(hBordersArray_);
  CommonValue vBordersArray=commonFactory.createCommonValue(vBordersArray_);
  CommonRecord labRecord=commonFactory.createCommonRecord((Schema)null);
  labRecord.setField("hBorders",hBordersArray);
  labRecord.setField("vBorders",vBordersArray);
  return labRecord;
}

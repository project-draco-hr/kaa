{
  if (o.getTenant() == null) {
    CTLSchemaMetaInfo metaInfo=o.getMetaInfo();
    CTLSchema ctlSchema=findByFqnAndVersion(metaInfo.getFqn(),metaInfo.getVersion());
    if (ctlSchema != null) {
      throw new RuntimeException("Can't save system ctl schema with same fqn and version.");
    }
  }
  return super.save(o);
}

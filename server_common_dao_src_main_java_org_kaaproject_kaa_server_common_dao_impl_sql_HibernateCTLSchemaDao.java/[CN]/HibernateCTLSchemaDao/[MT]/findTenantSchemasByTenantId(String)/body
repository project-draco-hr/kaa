{
  LOG.debug("Searching ctl schemas by tenant id [{}]",tenantId);
  List<CTLSchema> availableSchemas=findListByCriterionWithAlias(CTL_SCHEMA_META_INFO_PROPERTY,CTL_SCHEMA_META_INFO_ALIAS,Restrictions.and(Restrictions.eq(CTL_SCHEMA_TENANT_ID_ALIAS,Long.valueOf(tenantId)),buildScopeCriterion(CTLSchemaScopeDto.TENANT)));
  if (LOG.isTraceEnabled()) {
    LOG.trace("[{}] Search result: [{}].",tenantId,Arrays.toString(availableSchemas.toArray()));
  }
 else {
    LOG.debug("[{}] Search result: [{}].",tenantId,availableSchemas.size());
  }
  return availableSchemas;
}

{
  if (a1 == a2) {
    return 0;
  }
 else {
    if (a1 == null) {
      return -1;
    }
    if (a2 == null) {
      return 1;
    }
    if (a1.length == a2.length) {
      for (int i=0; i < a1.length; i++) {
        if (a1[i] != a2[i]) {
          return a1[i] - a2[i];
        }
      }
      return 0;
    }
 else {
      return a1.length - a2.length;
    }
  }
}

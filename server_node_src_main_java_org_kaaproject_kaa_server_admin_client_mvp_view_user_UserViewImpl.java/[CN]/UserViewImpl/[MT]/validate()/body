{
  String pattern="^(?:[a-zA-Z0-9_'^&/+-])+(?:\\.(?:[a-zA-Z0-9_'^&/+-])+)\" +\n" + "            \"*@(?:(?:\\\\[?(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?))\\\\.)\" +\n" + "            \"{3}(?:(?:25[0-5]|2[0-4][0-9]|[01]?[0-9][0-9]?)\\\\]?)|(?:[a-zA-Z0-9-]+\\\\.)\" +\n"+ "            \"+(?:[a-zA-Z]){2,}\\.?)$";
  RegExp regExp=RegExp.compile(pattern);
  boolean result=userName.getValue().length() > 0;
  result&=regExp.test(email.getValue());
  result&=authority.getValue() != null;
  return result;
}

{
  EndpointUser endpointUser=endpointUserDao.findByExternalIdAndTenantId(externalUid,tenantId);
  EndpointUserConfiguration endpointUserConfiguration=endpointUserConfigurationDao.findByUserIdAndAppTokenAndSchemaVersion(endpointUser.getId(),appToken,schemaVersion);
  return getDto(endpointUserConfiguration);
}

{
  if (that == null)   return false;
  boolean this_present_globalActor=true;
  boolean that_present_globalActor=true;
  if (this_present_globalActor || that_present_globalActor) {
    if (!(this_present_globalActor && that_present_globalActor))     return false;
    if (this.globalActor != that.globalActor)     return false;
  }
  return true;
}

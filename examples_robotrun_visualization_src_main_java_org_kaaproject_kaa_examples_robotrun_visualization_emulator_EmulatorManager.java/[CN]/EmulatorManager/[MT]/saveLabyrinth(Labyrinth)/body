{
  File labyrinthFile=new File(LABYRINTH_FILE);
  try {
    if (labyrinthFile.exists()) {
      labyrinthFile.delete();
    }
    FileOutputStream fos=new FileOutputStream(labyrinthFile);
    labyrinth.store(fos);
    fos.flush();
    fos.close();
  }
 catch (  IOException e) {
    LOG.error("Unable to save labyrinth to file [{}]!",labyrinthFile);
  }
}

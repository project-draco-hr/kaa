{
  String getUrl=composeURL(KAA_USER_CONFIGURATION_SERVLET_PATH,APPLICATION_ID_PARAMETER + "=" + URL.encodeQueryString(appId),USER_EXTERNAL_ID_PARAMETER + "=" + URL.encodeQueryString(externalUId),CONFIGURATION_SCHEMA_ID + "=" + URL.encodeQueryString(schemaVersion));
  String url=GWT.getModuleBaseURL() + getUrl;
  Window.open(url,"_self","enabled");
}

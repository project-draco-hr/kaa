{
  EndpointGroup endpointGroup=findById(id);
  Set<Topic> topics=endpointGroup.getTopics();
  Iterator<Topic> it=topics.iterator();
  while (it.hasNext()) {
    Topic topic=it.next();
    if (topic.getId() == Long.parseLong(topicId)) {
      it.remove();
      break;
    }
  }
  return save(endpointGroup);
}

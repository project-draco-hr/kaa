{
  StringBuilder builder=new StringBuilder();
  for (int i=0; i < vals.length; i++) {
    if (i % 2 == 0) {
      builder.append(vals[i]);
    }
 else {
      builder.append("=").append("\"").append(vals[i]).append("\"");
      if (i != vals.length - 1)       builder.append(",");
    }
  }
  return builder.toString();
}

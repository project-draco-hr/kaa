{
  Labyrinth labyrinth=null;
  if (labyrinthFile.exists()) {
    try {
      FileInputStream fis=new FileInputStream(labyrinthFile);
      labyrinth=BasicLabyrinth.load(fis);
    }
 catch (    Exception e) {
      LOG.error("Unable to load labyrinth from file [{}]!",labyrinthFile);
    }
  }
  return labyrinth;
}

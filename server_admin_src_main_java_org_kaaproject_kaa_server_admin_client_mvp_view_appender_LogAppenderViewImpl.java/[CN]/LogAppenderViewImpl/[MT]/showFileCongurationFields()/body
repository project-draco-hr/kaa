{
  publicKeyLabel=new Label(Utils.constants.publicKey());
  publicKeyLabel.addStyleName(REQUIRED);
  sshKey=new SizedTextBox(1000);
  sshKey.setWidth(FULL_WIDTH);
  sshKey.addInputHandler(this);

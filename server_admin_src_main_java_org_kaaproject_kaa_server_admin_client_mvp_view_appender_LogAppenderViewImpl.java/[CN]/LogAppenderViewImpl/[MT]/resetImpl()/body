{
  name.setValue("");
  status.setValue(false);
  description.setValue("");
  createdUsername.setValue("");
  createdDateTime.setValue("");
  if (metadatalistBox != null) {
    generateMetadataListBox();
  }
  if (type != null) {
    type.setValue(LogAppenderTypeDto.FILE,true);
  }
  getFooter().clear();
  if (sshKey != null) {
    sshKey.setValue("");
  }
}

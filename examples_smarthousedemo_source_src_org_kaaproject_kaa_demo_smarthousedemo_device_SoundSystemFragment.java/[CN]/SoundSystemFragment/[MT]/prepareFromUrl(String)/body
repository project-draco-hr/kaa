{
  long songId=Long.valueOf(url);
  Uri trackUri=ContentUris.withAppendedId(android.provider.MediaStore.Audio.Media.EXTERNAL_CONTENT_URI,songId);
  try {
    player.reset();
    player.setDataSource(mActivity,trackUri);
    player.prepare();
  }
 catch (  Exception e) {
    Log.e("Kaa","Unable to start playback for uri " + trackUri,e);
  }
}

{
  LOG.debug("Update server endpoint profile for endpoint with key hash {}, schema is {}",keyHash,schemaId);
  updateFirst(query(where(EP_ENDPOINT_KEY_HASH).is(keyHash).and(EP_SERVER_PROFILE_ID_PROPERTY).is(schemaId)),update(EP_SERVER_PROFILE_PROPERTY,serverProfile));
  return findById(ByteBuffer.wrap(keyHash));
}

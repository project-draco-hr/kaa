{
  LOG.info("BT notify: Disonnected");
synchronized (btSync) {
    BtConnected=false;
    if (stateCallback != null) {
      stateCallback.onDisconnected();
    }
  }
synchronized (waitSync) {
    recivedResponse=null;
    waitStatus=OperationStatus.FAILED;
    waitSync.notify();
    if (stateCallback != null) {
      stateCallback.onDisconnected();
    }
  }
}

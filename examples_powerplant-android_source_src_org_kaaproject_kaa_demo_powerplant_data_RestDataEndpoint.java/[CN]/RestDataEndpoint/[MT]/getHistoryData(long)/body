{
  try {
    HttpGet getRequest=new HttpGet(BASE_URL + "?from=" + fromTime);
    getRequest.addHeader("accept","application/json");
    JSONArray jsonArray=fetchJson(getRequest);
    return toDataReport(jsonArray);
  }
 catch (  Exception e) {
    Log.e(TAG,"Failed to fetch data",e);
  }
  return null;
}

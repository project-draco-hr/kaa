{
switch (field) {
case MAX:
    if (value == null) {
      unsetMax();
    }
 else {
      setMax((Long)value);
    }
  break;
case TOTAL:
if (value == null) {
  unsetTotal();
}
 else {
  setTotal((Long)value);
}
break;
case FREE:
if (value == null) {
unsetFree();
}
 else {
setFree((Long)value);
}
break;
}
}

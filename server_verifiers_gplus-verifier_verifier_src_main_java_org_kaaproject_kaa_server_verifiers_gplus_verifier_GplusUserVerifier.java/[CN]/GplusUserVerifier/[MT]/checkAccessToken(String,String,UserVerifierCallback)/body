{
  URL url=null;
  try {
    url=new URL(GOOGLE_OAUTH + URLEncoder.encode(accessToken,"UTF-8"));
  }
 catch (  MalformedURLException e) {
    LOG.debug("message",e);
  }
catch (  UnsupportedEncodingException e) {
    LOG.debug("message",e);
  }
  thredPool.submit(new RunnableVerifier(url,callback,userExternalId));
}

{
  LOG.debug("Find log schema by applicationId [{}] and version [{}] ",applicationId,version);
  LogSchema schema=null;
  if (isNotBlank(applicationId)) {
    schema=findOneByCriterionWithAlias(APPLICATION_PROPERTY,APPLICATION_ALIAS,Restrictions.and(Restrictions.eq(APPLICATION_REFERENCE,Long.valueOf(applicationId)),Restrictions.eq(MAJOR_VERSION_PROPERTY,version)));
  }
  return schema;
}

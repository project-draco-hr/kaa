{
  ControlApiCommand command=new ControlApiCommand("listApplications","list Applications by Tenant Id"){
    @Override public void runCommand(    CommandLine line,    ControlThriftService.Iface client,    PrintWriter writer,    PrintWriter errorWriter){
      listApplications(line,client,writer,errorWriter);
    }
  }
;
  Option opt=new Option("t",TENANT_ID,true,TENANT_ID_OPTION);
  opt.setRequired(true);
  command.addOption(opt);
  opt=new Option("o",OUTPUT,true,OUTPUT_FILE_TO_STORE_IDS);
  opt.setRequired(false);
  command.addOption(opt);
  return command;
}

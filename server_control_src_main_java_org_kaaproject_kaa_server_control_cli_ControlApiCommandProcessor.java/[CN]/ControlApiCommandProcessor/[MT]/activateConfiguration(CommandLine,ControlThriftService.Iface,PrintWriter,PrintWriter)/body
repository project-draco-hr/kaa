{
  try {
    if (line.hasOption("i")) {
      String configurationId=line.getOptionValue("i");
      @SuppressWarnings("unused") ConfigurationDto configuration=toDto(client.activateConfiguration(configurationId,null));
      writer.println("Configuration Activated.");
    }
  }
 catch (  TException e) {
    handleException(UNABLE_TO_ACTIVATE_CONFIGURATION,e,errorWriter);
  }
}

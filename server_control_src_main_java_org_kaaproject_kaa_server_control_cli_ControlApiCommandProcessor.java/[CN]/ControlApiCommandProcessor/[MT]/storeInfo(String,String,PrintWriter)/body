{
  try {
    File f=new File(file);
    f.getParentFile().mkdirs();
    BufferedWriter writer=new BufferedWriter(new FileWriter(f));
    writer.append(info);
    writer.flush();
    writer.close();
  }
 catch (  Exception e) {
    LOG.error("Unable to write Object Id to specified file '{}'! Error: {}",file,e.getMessage());
  }
}

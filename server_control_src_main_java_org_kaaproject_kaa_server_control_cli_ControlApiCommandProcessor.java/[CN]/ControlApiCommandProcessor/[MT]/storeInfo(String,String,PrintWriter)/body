{
  try {
    File f=new File(file);
    f.getParentFile().mkdirs();
    BufferedWriter writer=new BufferedWriter(new FileWriter(f));
    writer.append(info);
    writer.flush();
    writer.close();
  }
 catch (  Exception e) {
    errorWriter.println("Unable to write Object Id to specified file '" + file + ERROR+ e.getMessage());
  }
}

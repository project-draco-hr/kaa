{
  ControlApiCommand command=new ControlApiCommand("list" + type.getName(),"list " + type.getName() + "s"){
    @Override public void runCommand(    CommandLine line,    ControlThriftService.Iface client,    PrintWriter writer,    PrintWriter errorWriter){
      list(type,line,client,writer,errorWriter);
    }
  }
;
  Option opt=new Option("o",OUTPUT,true,OUTPUT_FILE_TO_STORE_IDS);
  opt.setRequired(false);
  command.addOption(opt);
  return command;
}

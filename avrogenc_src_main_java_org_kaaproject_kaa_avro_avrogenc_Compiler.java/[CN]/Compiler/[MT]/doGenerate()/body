{
  for (  Map.Entry<Schema,GenerationContext> cursor : schemaQueue.entrySet()) {
switch (cursor.getKey().getType()) {
case RECORD:
      processRecord(cursor.getKey());
    break;
case UNION:
  processUnion(cursor.getKey(),cursor.getValue());
break;
case ENUM:
processEnum(cursor.getKey());
break;
default :
break;
}
}
}

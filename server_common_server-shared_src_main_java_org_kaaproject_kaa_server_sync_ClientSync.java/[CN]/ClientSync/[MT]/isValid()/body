{
  ClientSyncMetaData md=this.getClientSyncMetaData();
  if (md.getProfileHash() == null) {
    ProfileClientSync profileRequest=this.getProfileSync();
    if (profileRequest != null && profileRequest.getEndpointPublicKey() != null) {
      return true;
    }
 else {
      return false;
    }
  }
 else {
    return true;
  }
}

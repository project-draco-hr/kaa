{
  flumeNodes=FlumeNodes.newBuilder().setFlumeNodes(Arrays.asList(new FlumeNode("localhost",12121),new FlumeNode("localhost",12122))).build();
  configuration.setHostsBalancing(flumeNodes);
  configuration.setCallbackThreadPoolSize(2);
  configuration.setClientsThreadPoolSize(2);

{
  LOG.debug("Remove notification by id {}",id);
  CassandraNotification nf=new CassandraNotification(id);
  Delete.Where deleteQuery=delete().from(getColumnFamilyName()).where(eq(NF_TOPIC_ID_PROPERTY,nf.getTopicId())).and(eq(NF_SCHEMA_VER_USER_TYPE_NAME,nf.getVersionType())).and(eq(NF_SEQ_NUM_PROPERTY,nf.getSeqNum()));
  LOG.trace("Remove notification by id {}",deleteQuery);
  execute(deleteQuery);
}

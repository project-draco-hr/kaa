{
  LOG.debug("Remove notifications by topic id {}",topicId);
  Delete.Where query=delete().from(getColumnFamilyName()).where(eq(CassandraModelConstants.NF_TOPIC_ID_PROPERTY,topicId)).and(QueryBuilder.in(CassandraModelConstants.NF_NOTIFICATION_TYPE_PROPERTY,getStringTypes(NotificationTypeDto.values())));
  execute(query);
  LOG.trace("Execute query {}",query);
}

{
  cliOut.reset();
  String cmdLine="listTopics -a " + applicationId;
  if (createOut) {
    cmdLine+=" -o dummy.obj";
  }
  int result=cli.processLine(cmdLine);
  Assert.assertEquals(result,0);
  String output=cliOut.toString("UTF-8");
  Assert.assertTrue(output.trim().startsWith("List of Topics:"));
}

{
  for (  EventSchemaVersion esv : versions) {
    int updateCount=0;
    if (ecBelongToThisFamilyVersion(ec,esv)) {
      updateCount=run.update(this.connection,"UPDATE " + EVENT_CLASS_TABLE_NAME + " SET events_class_family_versions_id=? WHERE id=?",esv.getId(),ec.getId());
      if (updateCount > 0)       ;
      break;
    }
  }
}

{
  kaaClient=Kaa.newClient(new DesktopKaaPlatformContext(),new ExampleSimpleKaaClientStateListener());
  kaaClient.addConfigurationListener(new ExampleConfigurationListener());
  kaaClient.start();
  displayConfiguration();
  try {
    System.in.read();
  }
 catch (  IOException e) {
    e.printStackTrace();
  }
  kaaClient.stop();
}

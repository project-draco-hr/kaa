{
  return deltaMemorizer.compute(key,new Computable<DeltaCacheKey,DeltaCacheEntry>(){
    @Override public DeltaCacheEntry compute(    DeltaCacheKey key){
      LOG.debug("Fetching result for getMergedConfiguration");
      DeltaCacheEntry result=worker.compute(key);
      setDelta(key,result);
      return result;
    }
  }
);
}

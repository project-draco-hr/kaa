{
  if (this == obj)   return true;
  if (obj == null)   return false;
  if (getClass() != obj.getClass())   return false;
  UserPlace other=(UserPlace)obj;
  if (userId == null) {
    if (other.userId != null)     return false;
  }
 else   if (!userId.equals(other.userId))   return false;
  return true;
}

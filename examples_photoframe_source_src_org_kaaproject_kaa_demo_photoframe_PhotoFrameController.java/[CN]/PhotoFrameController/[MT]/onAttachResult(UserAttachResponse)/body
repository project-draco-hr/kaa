{
  SyncResponseResultType result=response.getResult();
  if (result == SyncResponseResultType.SUCCESS) {
    mUserAttached=true;
    notifyRemoteDevices();
    mEventBus.post(new UserAttachEvent());
  }
 else {
    mUserAttached=false;
    String error=response.getErrorReason();
    mEventBus.post(new UserAttachEvent(error));
  }
}

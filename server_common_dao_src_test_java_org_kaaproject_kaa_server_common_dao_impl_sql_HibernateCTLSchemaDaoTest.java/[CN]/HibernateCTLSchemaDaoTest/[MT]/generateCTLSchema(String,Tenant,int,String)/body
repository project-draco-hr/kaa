{
  CTLSchema ctlSchema=new CTLSchema();
  ctlSchema.setMetaInfo(new CTLSchemaMetaInfo(fqn,version));
  if (isBlank(body)) {
    body=UUID.randomUUID().toString();
  }
  ctlSchema.setBody(body);
  if (tenant == null) {
    tenant=generateTenant();
  }
  ctlSchema.setTenant(tenant);
  return ctlSchema;
}

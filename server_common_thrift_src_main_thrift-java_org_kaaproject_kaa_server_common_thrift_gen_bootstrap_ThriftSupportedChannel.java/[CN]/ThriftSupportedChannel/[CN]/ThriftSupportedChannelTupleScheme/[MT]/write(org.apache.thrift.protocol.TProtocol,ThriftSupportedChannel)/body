{
  TTupleProtocol oprot=(TTupleProtocol)prot;
  BitSet optionals=new BitSet();
  if (struct.isSetType()) {
    optionals.set(0);
  }
  if (struct.isSetCommunicationParams()) {
    optionals.set(1);
  }
  oprot.writeBitSet(optionals,2);
  if (struct.isSetType()) {
    oprot.writeI32(struct.type.getValue());
  }
  if (struct.isSetCommunicationParams()) {
    struct.communicationParams.write(oprot);
  }
}

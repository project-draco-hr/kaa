{
  boolean correct=false;
  if (id != null) {
    String sid=id.getId();
    if (StringUtils.isNotBlank(sid)) {
      try {
        Long.valueOf(sid);
        correct=true;
      }
 catch (      NumberFormatException e) {
        LOG.debug("Exception while validating id. Can not parse id to long");
      }
    }
 else {
      correct=true;
    }
  }
  return correct;
}

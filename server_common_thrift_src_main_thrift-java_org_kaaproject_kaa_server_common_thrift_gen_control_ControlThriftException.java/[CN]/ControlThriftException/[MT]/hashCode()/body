{
  HashCodeBuilder builder=new HashCodeBuilder();
  boolean present_message=true && (isSetMessage());
  builder.append(present_message);
  if (present_message)   builder.append(message);
  boolean present_causeExceptionClass=true && (isSetCauseExceptionClass());
  builder.append(present_causeExceptionClass);
  if (present_causeExceptionClass)   builder.append(causeExceptionClass);
  boolean present_causeStackStrace=true && (isSetCauseStackStrace());
  builder.append(present_causeStackStrace);
  if (present_causeStackStrace)   builder.append(causeStackStrace);
  return builder.toHashCode();
}

{
  StringBuilder sb=new StringBuilder("ThriftEntityClusterAddress(");
  boolean first=true;
  sb.append("nodeId:");
  if (this.nodeId == null) {
    sb.append("null");
  }
 else {
    sb.append(this.nodeId);
  }
  first=false;
  if (!first)   sb.append(", ");
  sb.append("address:");
  if (this.address == null) {
    sb.append("null");
  }
 else {
    sb.append(this.address);
  }
  first=false;
  sb.append(")");
  return sb.toString();
}

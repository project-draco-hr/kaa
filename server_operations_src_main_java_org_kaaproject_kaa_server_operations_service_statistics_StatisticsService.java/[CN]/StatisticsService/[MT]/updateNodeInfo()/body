{
  if (config != null && operationsNode != null) {
    try {
      operationsNode.updateNodeStatsValues(ZkChannelsUtils.getZkChannelTypeFromChanneltype(getChannelType()),averageDeltaSync,averageProcessedRequests,averageOnlineSessions);
    }
 catch (    IOException e) {
      LOG.error("Error update statistics values on ZooKepper: " + e);
    }
catch (    ZkChannelException e) {
      LOG.error("Error statistics UpdateNodeInfo",e);
    }
  }
}

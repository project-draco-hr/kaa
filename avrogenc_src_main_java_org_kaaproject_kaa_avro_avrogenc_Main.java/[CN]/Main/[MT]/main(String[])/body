{
  try {
    if (args.length < 3) {
      throw new KaaCGeneratorException("Not enough argument. " + "Need {FULL_PATH_TO_SCHEMA} {OUTPUT_PATH} {SOURCE_NAME}");
    }
    Compiler compiler=new Compiler(args[0],args[1],args[2]);
    compiler.generate();
  }
 catch (  Exception e) {
    System.err.println("Compilation failure: " + e.toString());
  }
}

{
  checkAuthority(KaaAuthorityDto.TENANT_DEVELOPER,KaaAuthorityDto.TENANT_USER);
  try {
    checkApplicationId(applicationId);
    return toDtoList(clientProvider.getClient().findNotificationSchemasByAppIdAndType(applicationId,toGenericDataStruct(NotificationTypeDto.USER)));
  }
 catch (  Exception e) {
    throw Utils.handleException(e);
  }
}

{
  KaaAdmin.getDataSource().getUserNotificationSchemaInfosByApplicationId(applicationId,new AsyncCallback<List<SchemaInfoDto>>(){
    @Override public void onSuccess(    List<SchemaInfoDto> result){
      Collections.sort(result);
      SchemaInfoDto schemaInfo=result.get(0);
      detailsView.getNotificationSchemaInfo().setValue(schemaInfo,true);
      detailsView.getNotificationSchemaInfo().setAcceptableValues(result);
    }
    @Override public void onFailure(    Throwable caught){
      Utils.handleException(caught,detailsView);
    }
  }
);
}

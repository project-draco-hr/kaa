{
  if (schema.getType() == Type.RECORD) {
    for (    Field f : schema.getFields()) {
      Type type=f.schema().getType();
      if (type == Type.ARRAY || type == Type.BYTES || type == Type.STRING || type == Type.RECORD || type == Type.UNION) {
        return true;
      }
    }
  }
  return false;
}

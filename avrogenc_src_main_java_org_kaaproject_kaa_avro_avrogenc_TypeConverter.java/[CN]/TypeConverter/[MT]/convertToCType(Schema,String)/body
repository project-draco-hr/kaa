{
  String cType=new String();
switch (schema.getType()) {
case BOOLEAN:
    cType="KAA_BOOL";
  break;
case INT:
cType="int32_t";
break;
case LONG:
cType="int64_t";
break;
case STRING:
cType="char*";
break;
case BYTES:
cType="kaa_bytes_t*";
break;
case ARRAY:
cType="kaa_list_t*";
break;
case UNION:
cType="kaa_union_t*";
break;
case ENUM:
cType=namespace + "_" + StyleUtils.toLowerUnderScore(schema.getName())+ "_t";
break;
case RECORD:
cType=namespace + "_" + StyleUtils.toLowerUnderScore(schema.getName())+ "_t*";
break;
default :
break;
}
return cType;
}

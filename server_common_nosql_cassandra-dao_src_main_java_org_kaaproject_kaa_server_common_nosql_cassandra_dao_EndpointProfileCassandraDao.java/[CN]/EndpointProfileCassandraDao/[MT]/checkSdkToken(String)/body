{
  LOG.debug("Checking for endpoint profiles with SDK token {}",sdkToken);
  Statement query=QueryBuilder.select().from(CassandraModelConstants.EP_BY_SDK_TOKEN_COLUMN_FAMILY_NAME).where(QueryBuilder.eq(CassandraModelConstants.EP_BY_SDK_TOKEN_SDK_TOKEN_PROPERTY,sdkToken));
  return this.findOneByStatement(query) != null;
}

{
  FileKeyStoreService keyStoreService=new FileKeyStoreService();
  keyStoreService.setPrivateKeyLocation("private.key");
  keyStoreService.setPublicKeyLocation("public.key");
  String randomFolderName="randomFolder" + new Random().nextInt(100000);
  LOG.info("Creating random folder: {}",randomFolderName);
  System.setProperty("server_home_dir",randomFolderName);
  try {
    keyStoreService.loadKeys();
    Assert.assertNotNull(keyStoreService.getPrivateKey());
    Assert.assertNotNull(keyStoreService.getPublicKey());
    Files.delete(Paths.get(randomFolderName,keyStoreService.getPrivateKeyLocation()));
    Files.createFile(Paths.get(randomFolderName,keyStoreService.getPrivateKeyLocation()));
    keyStoreService.loadKeys();
  }
  finally {
    removeFolder(randomFolderName);
  }
}

{
  UserVerifier verifier=null;
  if (isNotBlank(appId)) {
    verifier=findOneByCriterionWithAlias(APPLICATION_PROPERTY,APPLICATION_ALIAS,Restrictions.and(Restrictions.eq(APPLICATION_REFERENCE,Long.valueOf(appId)),Restrictions.eq(USER_VERIFIER_TOKEN,verifierToken)));
    LOG.debug("Found log appender by application id {} and verifier token {}",appId,verifierToken);
  }
  return verifier;
}

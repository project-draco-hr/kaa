{
  List<Cell> result=new LinkedList<>();
  for (  Direction direction : Direction.values()) {
    BorderType border=current.getBorder(direction);
    if (border == BorderType.FREE) {
      Cell neighbor=getNeightbor(labyrinth,current,direction);
      if (neighbor != null) {
        result.add(neighbor);
      }
    }
  }
  return result;
}

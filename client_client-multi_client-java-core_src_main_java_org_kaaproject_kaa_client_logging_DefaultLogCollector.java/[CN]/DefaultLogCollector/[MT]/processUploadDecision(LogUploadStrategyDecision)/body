{
switch (decision) {
case UPLOAD:
    if (!isUploading) {
      isUploading=true;
      if (transport != null) {
        transport.sync();
      }
 else {
        LOG.warn("Log transport is null");
      }
    }
  break;
case CLEANUP:
storage.removeOldestRecord(configuration.getMaximumAllowedVolume());
break;
case NOOP:
default :
break;
}
}

{
  EventClassDto dto=createDto();
  dto.setId(getStringId());
  if (tenant != null) {
    dto.setTenantId(tenant.getStringId());
  }
  if (ecf != null) {
    dto.setEcfId(ecf.getStringId());
  }
  dto.setFqn(fqn);
  dto.setType(type);
  dto.setSchema(schema);
  dto.setVersion(version);
  return dto;
}

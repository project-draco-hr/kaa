{
  return updateLockedImpl(endpointUser.getVersion(),new Assignment[]{set(EP_USER_USERNAME_PROPERTY,endpointUser.getUsername()),set(EP_USER_ACCESS_TOKEN_PROPERTY,endpointUser.getAccessToken()),set(CassandraModelConstants.EP_USER_ENDPOINT_IDS_PROPERTY,endpointUser.getEndpointIds())},eq(EP_USER_EXTERNAL_ID_PROPERTY,endpointUser.getExternalId()),eq(EP_USER_TENANT_ID_PROPERTY,endpointUser.getTenantId()));
}

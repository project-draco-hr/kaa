{
  float prefWidth=0;
  prefWidth+=constructStringColumn(table,Utils.constants.schemaVersion(),new StringValueProvider<ConfigurationRecordDto>(){
    @Override public String getValue(    ConfigurationRecordDto item){
      return item.getSchemaVersion() + "";
    }
  }
,80);
  prefWidth+=super.constructColumnsImpl(table);
  return prefWidth;
}

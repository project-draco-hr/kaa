{
  try {
    nodePath=client.create().withMode(CreateMode.EPHEMERAL).forPath(ControlNodeTracker.CONTROL_SERVER_NODE_PATH,controlNodeAvroConverter.get().toByteArray(currentNodeInfo));
  }
 catch (  NodeExistsException e) {
    LOG.info("master already exists");
  }
catch (  Exception e) {
    LOG.error("Unknown Error",e);
    close();
    throw new IOException(e);
  }
  return true;
}
